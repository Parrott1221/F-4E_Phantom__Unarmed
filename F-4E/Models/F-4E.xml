 <!-- Rotating beacon flash -->
 <animation>
  <type>material</type>
  <object-name>Beacon</object-name>
  <emission>
    <factor-prop>sim/multiplay/generic/float[0]</factor-prop>
    <red>1</red>
    <green>0</green>
    <blue>0</blue>
  </emission>
 </animation>

