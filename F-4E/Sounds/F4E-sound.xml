<?xml version="1.0"?>
 
<PropertyList>

 <fx>

  <engine1>
   <name>engine1</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/milpower.wav</path>
   <condition>
    <property>/engines/engine[0]/running</property>
    <not>
      <property>/sim/current-view/internal</property>
    </not>
   </condition>
   <position>
    <x>4.0</x>
    <y>-1.0</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>100.0</inner-angle>
    <outer-angle>180.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>/controls/engines/engine[0]/throttle</property>
    <factor>0.5</factor>
    <min>0.1</min>
    <max>0.5</max>
   </volume>
  </engine1>

  <engine2>
   <name>engine2</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/milpower.wav</path>
   <condition>
    <property>/engines/engine[1]/running</property>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
   </condition>
   <position>
    <x>4.0</x>
    <y>1.0</y>
    <z>0.0</z>
   </position>
   <orientation>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
    <inner-angle>100.0</inner-angle>
    <outer-angle>180.0</outer-angle>
    <outer-gain>0.2</outer-gain>
   </orientation>
   <volume>
    <property>/controls/engines/engine[1]/throttle</property>
    <factor>0.5</factor>
    <min>0.1</min>
    <max>0.5</max>
   </volume>
  </engine2>

  <engine1cockpit>
   <name>engine1-from-cockpit</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/milpower.wav</path>
   <condition>
    <property>/engines/engine[0]/running</property>
     <property>/sim/current-view/internal</property>
   </condition>
   <volume>
    <property>/controls/engines/engine[0]/throttle</property>
    <factor>0.2</factor>
    <min>0.05</min>
    <max>0.2</max>
   </volume>
  </engine1cockpit>

  <engine2cockpit>
   <name>engine2-from-cockpit</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/milpower.wav</path>
   <condition>
    <property>/engines/engine[1]/running</property>
     <property>/sim/current-view/internal</property>
   </condition>
   <volume>
    <property>/controls/engines/engine[1]/throttle</property>
    <factor>0.2</factor>
    <min>0.05</min>
    <max>0.2</max>
   </volume>
  </engine2cockpit>

  <engine-burner>
   <name>burner</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/afterburner.wav</path>
   <condition>
    <or>
    <property>/engines/engine[0]/augmentation</property>
    <property>/engines/engine[1]/augmentation</property>
    </or>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
   </condition>
   <orientation>
    <x>0</x>
    <y>0</y>
    <z>1</z>
    <inner-cone>160.0</inner-cone>
    <outer-cone>180.0</outer-cone>
    <outer-gain>0.5</outer-gain>
   </orientation>
   <volume>
    <property>/controls/throttle[0]</property>
    <factor>1</factor>
    <min>0.4</min>
    <max>0.5</max>
   </volume>
  </engine-burner>

  <engine-burner-cockpit>
   <name>burner-from-cockpit</name>
   <mode>looped</mode>
   <path>Aircraft/DavePack/Sounds/afterburner.wav</path>
   <condition>
    <or>
     <property>/engines/engine[0]/augmentation</property>
     <property>/engines/engine[1]/augmentation</property>
    </or>
    <property>/sim/current-view/internal</property>
   </condition>
   <volume>
    <property>/controls/throttle[0]</property>
    <factor>0.1</factor>
    <min>0.1</min>
    <max>0.1</max>
   </volume>
  </engine-burner-cockpit>

  <engine1-whine>
   <name>compressor-whine-external-engine0</name>
   <path>Sounds/turbojet_fan.wav</path>
   <mode>looped</mode>
   <condition>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
    <greater-than>
      <property>/engines/engine[0]/n2</property>
      <value>1</value>
    </greater-than>
   </condition>
   <position>     <!-- meters -->
    <x>-4.0</x>   <!-- +X = aft  -->
    <y> 0.6</y>   <!-- +Y = left -->
    <z> 0.0</z>   <!-- +Z = up   -->
   </position>
   <orientation>  <!-- normalized vector -->
    <x>-1.0</x>   <!-- +X = aft  -->
    <y> 0.0</y>   <!-- +Y = left -->
    <z> 0.0</z>   <!-- +Z = up   -->
    <inner-angle>50.0</inner-angle>
    <outer-angle>100.0</outer-angle>
    <outer-gain>0.4</outer-gain>
   </orientation>
   <pitch>
    <property>/engines/engine[0]/n2</property>
    <offset>0.7</offset>
    <factor>.004</factor>
   </pitch>
   <volume>
    <property>/engines/engine[0]/n2</property>    
    <offset>0.3</offset>
    <factor>-0.003</factor>
    <max>0.3</max>
   </volume>
  </engine1-whine>

  <engine2-whine>
   <name>compressor-whine-external-engine1</name>
   <path>Sounds/turbojet_fan.wav</path>
   <mode>looped</mode>
   <condition>
    <not>
     <property>/sim/current-view/internal</property>
    </not>
    <greater-than>
      <property>/engines/engine[1]/n2</property>
      <value>1</value>
    </greater-than>
   </condition>
   <position>     <!-- meters -->
    <x>-4.0</x>   <!-- +X = aft  -->
    <y>-0.6</y>   <!-- +Y = left -->
    <z> 0.0</z>   <!-- +Z = up   -->
   </position>
   <orientation>  <!-- normalized vector -->
    <x>-1.0</x>   <!-- +X = aft  -->
    <y> 0.0</y>   <!-- +Y = left -->
    <z> 0.0</z>   <!-- +Z = up   -->
    <inner-angle>50.0</inner-angle>
    <outer-angle>100.0</outer-angle>
    <outer-gain>0.4</outer-gain>
   </orientation>
   <pitch>
    <property>/engines/engine[1]/n2</property>
    <offset>0.7</offset>
    <factor>0.004</factor>
   </pitch> 
   <volume>
    <property>/engines/engine[1]/n2</property>
    <offset>0.3</offset>
    <factor>-0.003</factor>
    <max>0.3</max>
   </volume>
  </engine2-whine>

  <tire-rumble>
   <name>tire-rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
     <property>/sim/current-view/internal</property>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.004</factor>
    <max>0.3</max>
   </volume>
  </tire-rumble>

  <gear-wind>
   <name>gear-wind</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
     <property>/sim/current-view/internal</property>
     <greater-than>
      <property>/gear/gear[1]/position-norm[0]</property>
      <value>0.01</value>
     </greater-than>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.002</factor>
    <max>1.0</max>
   </volume>
  </gear-wind>

  <squeal-gear>
   <name>squeal</name>
   <path>Sounds/squeal.wav</path>
   <mode>once</mode>
   <condition>
    <and>
      <property>gear/gear[1]/wow</property>
      <property>gear/gear[2]/wow</property>
      <property>fdm/jsbsim/systems/ADC/airborne</property>
    </and>
   </condition>
  </squeal-gear>

  <wind>
   <name>wind</name>
   <mode>looped</mode>
   <path>Sounds/wind.wav</path>
   <condition>
     <property>/sim/current-view/internal</property>
   </condition>
   <property>/velocities/airspeed-kt</property>
   <volume>
    <property>/fdm/jsbsim/aero/qbar-psf</property>
    <factor>0.0002</factor>
    <min>0.0</min>
    <max>0.3</max>
   </volume>
   <pitch>
    <property>/fdm/jsbsim/aero/qbar-psf</property>
    <factor>0.002</factor>
    <offset>1.25</offset>
   </pitch>
  </wind>

  <alpha-buffet>
   <name>alpha-buffet</name>
   <mode>looped</mode>
   <condition>
    <property>/sim/current-view/internal</property>
    <greater-than>
     <property>/fdm/jsbsim/aero/alpha-deg</property>
     <value>9.0</value>
    </greater-than>
    <greater-than>
     <property>velocities/airspeed-kt</property>
     <value>50.0</value>
    </greater-than>
   </condition>
   <path>Sounds/rumble.wav</path>
   <volume>
    <property>/fdm/jsbsim/aero/alpha-deg</property>
    <factor>0.263</factor>
    <max>1.0</max>
   </volume>
   <pitch>
    <property>/fdm/jsbsim/aero/alpha-deg</property>
    <factor>0.00005</factor>
   </pitch>
  </alpha-buffet>

  <speedbrake-buffet>
   <name>speedbrake</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
     <property>/sim/current-view/internal</property>
     <property>/surface-positions/speedbrake-pos-norm</property>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <factor>0.0025</factor>
    <max>0.3</max>
   </volume>
  </speedbrake-buffet>

  <!-- gun -->
  <guns>
   <name>gunfire</name>
   <mode>looped</mode>
   <condition>
    <property>controls/armament/trigger</property>
   </condition>
   <path>Aircraft/DavePack/Sounds/20mm-gun.wav</path>
  </guns>

  <crash>
   <name>boing</name>
   <condition>
    <property>fdm/jsbsim/systems/crash-detect/crashed</property>
   </condition>
   <path>Aircraft/DavePack/Sounds/boing.wav</path>
  </crash>


</fx>
 
</PropertyList>
